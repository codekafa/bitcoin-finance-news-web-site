
@using BTC.Model.Entity

@model List<UserProducts>


<table class="table table-bordered">

    <thead>
        <tr>
            <th>
               Ad
            </th>
            <th>
              Url
            </th>
            <th>
               Fiyat
            </th>
            <th>
               Yayında mı?
            </th>
            <th>
                İşlemler
            </th>
        </tr>
    </thead>

    <tbody id="productListTableBody">
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Name</td>
            <td>@item.Uri</td>
            <td>@item.Price</td>
            <td>@item.IsPublish</td>
            <td><a href="~/urun-duzenle/@item.ID" target="_blank" class="btn btn-xs btn-primary">Düzenle</a></td>
        </tr>
        }
    </tbody>

</table>
